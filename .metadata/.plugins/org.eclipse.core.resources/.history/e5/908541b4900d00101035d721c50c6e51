import crawler.GoldCrawler;
import driver.AMyDriverContext;
import driver.ChromeDriverContext;

public class Main {
	public static void main(String[] args) {
		AMyDriverContext myDriver = new ChromeDriverContext();
        
        // Instantiate and execute the crawler
        GoldCrawler goldCrawler = new GoldCrawler(driverContext);
        boolean success = goldCrawler.crawl();
        
        // Output the result of the crawling operation
        if (success) {
            System.out.println("Crawling completed successfully.");
